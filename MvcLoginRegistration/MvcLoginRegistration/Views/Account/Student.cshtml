    @model IEnumerable<MvcLoginRegistration.Models.UserAccount>*@
@{
    ViewBag.Title = "Student";
}

<h2>Student</h2>
@if (Session["UserId"] != null)
{
    <h4> Hello @Session["Username"].ToString()</h4>
}


<dl class="dl-horizontal">
    <div>
        @Html.ActionLink("Click here to Upload your image", "Add", "Image")

    </div>



    <div>
        @Html.ActionLink("Update your profile.", "Details", "studentlistcrud")

    </div>

    <div class="form-group">
        @Html.Label("Choose your course", htmlAttributes: new { @class = "control-label col-md-2" })
        @Html.DropDownList("Choose your course",new List<SelectListItem>
              {
                new SelectListItem{Text="BE(IT)",Value="BE(IT)",Selected=true},
                new SelectListItem{Text="BE(EC)",Value="BE(EC)"},

               new SelectListItem{Text="BE(Mechanical)",Value="BE(Mechanical)"}

              },"Select Course")

    </div>




